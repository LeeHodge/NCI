[client]
port=33066
user=drupaluser

[mysql]


[mysqladmin]
user=root
port=33066


[mysqld]
pid-file="D:/acquia-drupal/mysql/data/mysql.pid"
log-error="D:/acquia-drupal/mysql/data/mysql.err"

# The TCP/IP Port the MySQL Server will listen on
port=33066

#Path to installation directory. All paths are usually resolved relative to this.
basedir="D:/acquia-drupal/mysql"

#Path to the database root
datadir="D:/acquia-drupal/mysql/data"

#set innodb as default
default-storage-engine=InnoDB

#Max packets
max_allowed_packet = 128M

#The log buffer is written out to the file at each commit. Flushing on the log file takes place once per second 
innodb_flush_log_at_trx_commit=2
